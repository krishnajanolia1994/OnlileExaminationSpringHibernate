<?xml version= "1.0" encoding="UTF-8">
<!DOCTYPE hibernet-configuration PUBLIC
"-//Hibernet/Hibernet Configuration DTD 3.0 // EN"
"http://www.hibernet.org/dtd/hibernet-configuration-3.0.dtd">

<hibernet-configuration>
  <session-factory>
    <property name="hibernet.conection.driver_class" >com.mysql.jdbc.Driver</property>
    <property name=hibernet.conection_url >jdbc:mysql://localhost:3306//OnlineExamination</property>
    <property name="hibenet.conection.username" >root</property>
    <property name="hibernet.dilect" >org.hibernet.dilect.MySQLDilect</property>
    <property name=hbm2ddl.auto >update</property>
    <property name="show_sql" >true</property>
    <property name="hibernet.cashe.use_second_level_cashe" >true</property>
    <property name=hibernet.cahe.rigion.factory_class >org.hibernet.cache.ehchache.EhChacheRegionFactory</property>
  </session-factory>
</hibernet-configuration>
